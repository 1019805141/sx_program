/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.container2{
  flex-direction: column;
  justify-content: space-between;
  padding: 0 30rpx;
  box-sizing: border-box;
  height: 100%;
}
.jiange{
  background: #f2f2f2;
  height: 12rpx;
  width: 100%;
  clear: both;
}
.index_title{
  overflow: hidden;
  padding: 34rpx 0;
}
.index_title_left{
  position: relative;
  float: left;

}
.index_title_left p{
  display: inline-block;
  z-index: 1000;
  font-size: 38rpx;
  font-weight: bold;
  padding-left: 10rpx;
}
.index_title_left>image{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left: 0;
  z-index: -1000;
}
.index_title_right{
  float: right;
  color: #4c4c4c;
  font-size: 28rpx;
}

/* 筛选 */
.page {
  font-family: 'PingFang SC',
                 'Helvetica Neue',
                 Helvetica,
                 'Droid Sans Fallback',
                 'Microsoft Yahei',
                 sans-serif;
  background: #fff;
  overflow: hidden;
  position: absolute;
  width: 90%;
  left: 5%;
  top:7%
}

.nav {
  position: relative;
  z-index: 99;
  display: flex;
  border-bottom: 1rpx solid #f4f4f4;
  background: #fff;
  border-radius:20rpx 20rpx 0 0;
  
}

.nav-son {
  display: flex;
  flex: 1;
  text-align: center;
  height: 80rpx;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
}

.borders {
  border-left: 1rpx solid #f4f4f4;
  border-right: 1rpx solid #f4f4f4;
}

.content {
  display: inline-block;
  font-size: 30rpx;
  color: #666;
}

.icon {
  display: inline-block;
  border: 10rpx solid transparent;
  border-top: 10rpx solid #666;
  margin-left: 10rpx;
  margin-top: 10rpx;
}

.temp {
  position: relative;
  z-index: 4;
  background: #fff;
}

.temp1 {
  display: none;
  width: 100%;
  max-height: 550rpx;
  overflow-y: scroll;
  line-height: 100rpx;
}

.temp2 {
  display: none;
  width: 100%;
  max-height: 750rpx;
  overflow-y: scroll;
  line-height: 100rpx;
  background: #fff;
  border-radius: 0 0 20rpx 20rpx;

}

.slidedown {
  transform: translateY(0%);
}

.temp view {
  /* border-bottom: 1rpx solid #d1d3d4; */
  font-size: 28rpx;
  color: #666;
}

.pxTable {
  float: left;
  text-align: center;
  font-size: 26rpx;
  color: #808080;
  width: 19%;
}

.quyu {
  position: relative;
  /* 区域的筛选：默认状态下不显示 */
  display: none;
  /* height: 450rpx; */
  background: #ffffff;
  padding: 30rpx 0;
  z-index: 5;
  border-radius: 0 0 20rpx 20rpx;
}

.quyu .qy {
  overflow-y: scroll;
  float: left;
  width: 33.33%;
  /* height: 450rpx; */
  height: 82%;
  line-height: 80rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #717273;
  border-right: 1rpx solid #f4f4f4;
}

.qy view {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  padding-left: 45rpx;
  border-bottom: 1rpx solid #f4f4f4;
}

.quyu-left {
  background: #fff;
}

.quyu-center {
  background: #fff;
}

.quyu-right {
  background: #f5f5f5;
}

.quyu-right view {
  border-bottom: 1rpx solid #efefef;
  display: flex;
  align-items: center;
}

.quyu-right image {
  width: 34rpx;
  height: 34rpx;
  margin-left: 30rpx;
}

.current {
  /* background: #e0e0e0; */
  color: #ec603d;
}

.current2 {
  /* background: #fff; */
  color: #ec603d;
}

.current3 {
  /* background: #fff; */
  color: #ec603d;
}

.fullbg {
  position: fixed;
  top: 0;
  z-index: 1;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0,0.6);
  transition: all 0.5s;
  display: none;
  left: 0;
}

.fullopacity {
  display: block;
}

.nav-son.active .content {
  color: #0DA6FF;
}

.nav-son.active .icon {
  border: 10rpx solid transparent;
  border-top: 10rpx solid #0DA6FF;
}

@keyframes slidown {
  from {
    transform: translateY(-100%);
  }

  to {
    transform: translateY(0%);
  }
}

.slidown {
  display: block;
  animation: slidown 0.3s ease-in both;
}

@keyframes slidup {
  from {
    transform: translateY(0%);
  }

  to {
    transform: translateY(-100%);
  }
}

.slidup {
  display: block;
  animation: slidup 0.3s ease-in both;
}

.disappear {
  display: none;
}

.form-btn {
  width: 100%;
  display: flex;
  height: 98rpx;
}

.btn-reset {
  background-color: #f9f9f9;
}

.btn-submit {
  background-color: #e35b27;
  color: #fff;
}

.sliderHView {
  margin: 0 40rpx 0 40rpx;
  height: 250rpx;
  width: 70%;
  margin: auto;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  background-color: #fff;
}

.valueSty {
  font-size: 30rpx;
  text-align: center;
  color: #999;
  margin-top: 15rpx;
}

.showMoney text {
  margin-right: 30rpx;
}

.twoSlider {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.contentCenter {
  text-align: center;
}
.current4 {
  /* background: #fff; */
  color: #0DA6FF !important;

}
.quyulist{
  overflow: hidden;
}
.quyulist_left{
  float: left;
  width: 20%;
  border-right: 1px solid #E6E6E6;
  text-align: center;
  font-weight: bold;
  padding-bottom: 50rpx;
  color: #808080;
  overflow-y: scroll;
  height: 500rpx;
}
.quyulist .active_left{
  color: #0DA6FF;
}
.quyulist_right{
  float: left;
  width: 78%;
  overflow: hidden;
}
.quyulist_right>view{
  float: left;
  width: 25%;
  text-align: center;
  color: #808080;
}
.quyulist_right .active_right{
  color: #0DA6FF;
}
.quyulist_right>view{
  float: left;
  width: 30%;
  text-align: center;
  font-size: 28rpx;
  color: #808080;
  margin-bottom: 40rpx;
}
.quyulist_right>.quyulist_right_all{
  width: 100%!important;
  text-align: left!important;
  padding-left: 50rpx;

}
.bottom_footer{
  background: #f5f5f5;
  text-align: center;
  color: #777777;
  padding: 20rpx 0;
}