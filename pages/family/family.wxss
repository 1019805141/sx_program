/* pages/family/family.wxss */
.banner image {
    width: 750rpx;
    height: 400rpx;
}

.list {
    padding: 14rpx 30rpx;
    border: 1px solid #E6E6E6;
    overflow: hidden;
    margin-bottom: 20rpx;
}

.list>image {
    width: 130rpx;
    height: 130rpx;
    float: left;
    margin-right: 20rpx;
}

.list_text>view {
    font-size: 32rpx;
    color: #262626;
    font-weight: 500;
    white-space: nowrap;
    text-overflow: ellipsis;
    -webkit-text-overflow: ellipsis;
    overflow: hidden;
    margin-bottom: 20rpx;
    margin-top: 10rpx;
}

.list_text>p {
    color: #808080;
    float: left;
    margin-right: 14rpx;
    font-size: 26rpx;
}

.list_text>p>image {
    width: 22rpx;
    height: 22rpx;
    margin-right: 10rpx;
}