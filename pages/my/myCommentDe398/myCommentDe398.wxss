.kecheng{
    background: #0da6ff;
    text-align: center;
    height: 74rpx;
    border-radius: 20rpx;
    margin-top: 30rpx;
    margin-bottom: 30rpx;
}
.kecheng>i{
    width:180rpx ;
    height: 1px;
    background: #ffffff;
    display: inline-block;
    margin: 0 20rpx;
    margin-bottom: 10rpx;
}
.kecheng>view{
    display: inline-block;
    color: #ffffff;
    line-height: 74rpx;
}

.box{
    border: 1px solid #E6E6E6;
    margin-bottom: 30rpx;
    position: relative;
}
.box_top{
    padding: 20rpx;
    overflow: hidden;
}
.box_top>image{
    width: 130rpx;
    height: 130rpx;
    display: block;
    float: left;
    margin-right: 20rpx;
}
.box_top_text{
    float: left;
    width: 76%;
}
.box_top_text>view{
    font-size: 30rpx;
    color: #262626;
}
.box_top_text>p{
    color: #808080;
    font-size: 26rpx;
    margin-top: 10rpx;
    display: block;
}
.daitiyan{
    float: right;
    font-size: 28rpx;
    color: #0DA6FF;
    border-radius: 6rpx;
    margin-top: 40rpx;
}
.yiwancheng{
    float: right;
    font-size: 28rpx;
    color: #893BFF;
    border-radius: 6rpx;
    margin-top: 40rpx;
}
.daifukuan{
    float: right;
    font-size: 28rpx;
    color: #FF3B0D;
    border-radius: 6rpx;
    margin-top: 40rpx;
}
.box_bottom{
    padding: 10rpx 0;
    border-top: 1px solid #E6E6E6;
    padding-left: 30rpx;
}
.box_bottom>image{
    width: 20rpx;
    height: 30rpx;
    float: left;
    display: block;
    margin-right: 15rpx;
    margin-top: 6rpx;
}
.box_bottom>view{
    color: #808080;
    font-size: 28rpx;
}
.pingfen{
    overflow: hidden;
    margin-bottom: 30rpx;
}
.pingfen_title{
    float: left;
    font-size: 28rpx;
    font-weight: bold;
    margin-right: 20rpx;
}
.star{
    float: left;
    margin-right: 20rpx;
}
.star>image{
    width: 21rpx;
    height: 21rpx;
    display: inline-block;
    margin-right: 5rpx;margin-bottom: 5rpx;

}
.pingfen_fen{
    color: #666666;
    font-size: 28rpx;
    float: left;
}
.pingjia{
    background: #F2F2F2;
    padding: 30rpx 0;
}
.pingjia view{
    color: #666666;
    font-size: 28rpx;
    margin-bottom: 10rpx;
}
.pingjia view>p{
    color: #333333!important;
}
.img_group{
    margin-top: 30rpx;
}
.img_group>image{
    width: 180rpx;
    height: 180rpx;
    border-radius: 10rpx;
    display: inline-block;
    margin-right: 20rpx;
}
.time{
    margin-top: 30rpx;
    color:#4D4D4D;
    font-size: 28rpx;
}