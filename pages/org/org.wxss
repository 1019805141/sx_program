    margin-bottom: 30rpx;
}
.teacher>image{
    width: 130rpx;
    height: 130rpx;
    border-radius: 10rpx;
    margin-right: 20rpx;
    float: left;
}
.teacher_right{
    float: left;
}
.teacher_right>view{
    font-size: 30rpx;
    color: #262626;
